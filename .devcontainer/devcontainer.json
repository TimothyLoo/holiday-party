{
  "name": "Next.js Holiday Party Dev Container",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "holiday-party-nextjs",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "eslint.enable": true,
        "typescript.tsdk": "node_modules/typescript/lib",
        "git.enabled": true
      }
    }
  },
  "forwardPorts": [4444],
  "remoteUser": "root"
}